class Employee
{
 int emp_id;
String emp_name;
double emp_sal;
Employee(int a,String b,double c)
{
emp_id=a;
emp_name=b;
emp_sal=c;
return;
}
public static void main(String[] args)
{
Employee E1= new Employee(123,"abhi",0.5);
System.out.println(E1.emp_id);
System.out.println(E1.emp_name);
System.out.println(E1.emp_sal);
}
}
